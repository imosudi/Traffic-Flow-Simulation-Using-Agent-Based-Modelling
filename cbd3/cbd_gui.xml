<viewsettings>
    <scheme name="real world"/>
    <delay value="50"/>
    <viewport zoom="100" x="200" y="100" angle="0"/>
    <decal file="background.png" centerX="200" centerY="100" width="800" height="600" rotation="0"/>
    <background backgroundColor="240,240,240" showGrid="1" gridXSize="100.00" gridYSize="100.00"/>
    <edges laneEdgeMode="0" laneShowBorders="1" showLinkDecals="1" showRails="1" hideConnectors="0"
           edgeName_show="0" edgeName_size="50.00" edgeName_color="orange" 
           internalEdgeName_show="0" internalEdgeName_size="40.00" internalEdgeName_color="128,64,0" 
           cwaEdgeName_show="0" cwaEdgeName_size="50.00" cwaEdgeName_color="magenta" 
           streetName_show="1" streetName_size="55.00" streetName_color="yellow">
        <colorScheme name="uniform">
            <entry color="black"/>
        </colorScheme>
        <colorScheme name="by selection">
            <entry color="grey" name="unselected"/>
            <entry color="0,80,180" name="selected"/>
        </colorScheme>
        <colorScheme name="by speed">
            <entry color="red" threshold="0.00"/>
            <entry color="yellow" threshold="8.33"/>
            <entry color="green" threshold="15.28"/>
            <entry color="cyan" threshold="22.22"/>
            <entry color="blue" threshold="33.33"/>
            <entry color="magenta" threshold="41.67"/>
        </colorScheme>
    </edges>
    <vehicles vehicleMode="0" vehicleQuality="0" vehicle_minSize="1.00" vehicle_exaggeration="1.00" vehicle_showBlinker="1"
             vehicleName_show="0" vehicleName_size="50.00" vehicleName_color="204,153,0">
        <colorScheme name="by type">
            <entry color="blue" name="car"/>
            <entry color="red" name="bus"/>
            <entry color="green" name="bicycle"/>
            <entry color="yellow" name="pedestrian"/>
        </colorScheme>
        <colorScheme name="by speed">
            <entry color="red" threshold="0.00"/>
            <entry color="yellow" threshold="8.33"/>
            <entry color="green" threshold="15.28"/>
            <entry color="cyan" threshold="22.22"/>
            <entry color="blue" threshold="33.33"/>
            <entry color="magenta" threshold="41.67"/>
        </colorScheme>
    </vehicles>
    <pois poi_minSize="0.00" poi_exaggeration="1.00" poi_showName="0" poi_nameSize="50.00"/>
    <legend showSizeLegend="1"/>
</viewsettings>