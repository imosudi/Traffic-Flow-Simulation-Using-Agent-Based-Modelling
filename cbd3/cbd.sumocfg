
<configuration>
    <input>
        <net-file value="cbd.net.xml"/>
        <route-files value="cbd.rou.xml"/>
        <additional-files value="cbd_detectors.add.xml"/>
        
    </input>
    <time>
        <begin value="0"/>
        <end value="21600"/>
        <step-length value="0.5"/>
    </time>
    <processing>
        <lateral-resolution value="0.8"/>
        <ignore-junction-blocker value="60"/>
        <collision.action value="warn"/>
        <time-to-teleport value="120"/>
    </processing>
    <report>
        <verbose value="true"/>
        <duration-log.statistics value="true"/>
        <log value="simulation.log"/>
    </report>
    <gui_only>
        <gui-settings-file value="cbd_gui.xml"/>
        <start value="true"/>
        <quit-on-end value="false"/>
    </gui_only>
</configuration>